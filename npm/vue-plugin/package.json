{
  "name": "@bgql/vue-plugin",
  "version": "0.1.0",
  "description": "Vue SFC plugin for Better GraphQL - supports <script lang=\"gql\">",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./vite": {
      "import": "./dist/vite.js",
      "types": "./dist/vite.d.ts"
    },
    "./composables": {
      "import": "./dist/composables.js",
      "types": "./dist/composables.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "test": "vitest",
    "typecheck": "tsgo --noEmit",
    "lint": "oxlint --type-aware",
    "fmt": "oxfmt --write src"
  },
  "keywords": [
    "graphql",
    "vue",
    "sfc",
    "vite",
    "plugin"
  ],
  "license": "MIT",
  "peerDependencies": {
    "vue": "^3.0.0",
    "@vue/compiler-sfc": "^3.0.0",
    "vite": "^5.0.0 || ^6.0.0"
  },
  "peerDependenciesMeta": {
    "vite": {
      "optional": true
    },
    "@vue/compiler-sfc": {
      "optional": true
    }
  },
  "dependencies": {
    "@bgql/sdk": "*"
  },
  "devDependencies": {
    "oxfmt": "^0.1.0",
    "oxlint": "^0.17.0",
    "@typescript/native-preview": "^7.0.0-dev.20250128.1",
    "typescript": "^5.0.0",
    "tsdown": "^0.6.0",
    "vitest": "^3.0.0",
    "vue": "^3.4.0",
    "@vue/compiler-sfc": "^3.4.0",
    "vite": "^6.0.0"
  }
}
