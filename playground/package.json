  },
  "devDependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@typescript/native-preview": "^7.0.0-dev.20250122",
    "oxfmt": "^0.27.0",
    "oxlint": "^1.42.0",
    "@vitejs/plugin-vue": "^6.0.3",
    "gh-pages": "^6.2.0",
    "sass": "^1.83.4",
    "tsdown": "^0.20.1",
    "typescript": "~5.9.3",
    "vite": "8.0.0-beta.10",
    "vue-tsc": "^3.2.4"
  }
